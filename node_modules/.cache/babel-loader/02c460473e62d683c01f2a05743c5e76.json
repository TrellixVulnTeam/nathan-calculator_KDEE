,OAAA;;AACA,WAAA,WAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,IAAA;AACA,KATA;AAUA,IAAA,KAVA,iBAUA,IAVA,EAUA;AACA,WAAA,QAAA,GAAA,UAAA,CAAA,EAAA,CAAA;AAAA,eAAA,CAAA,GAAA,CAAA;AAAA,OAAA;;AACA,WAAA,WAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,IAAA;AACA,KAdA;AAeA,IAAA,KAfA,iBAeA,IAfA,EAeA;AACA,WAAA,QAAA,GAAA,UAAA,CAAA,EAAA,CAAA;AAAA,eAAA,CAAA,GAAA,CAAA;AAAA,OAAA;;AACA,WAAA,WAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,IAAA;AACA,KAnBA;AAoBA,IAAA,QApBA,oBAoBA,IApBA,EAoBA;AACA,WAAA,QAAA,GAAA,UAAA,CAAA,EAAA,CAAA;AAAA,eAAA,CAAA,GAAA,CAAA;AAAA,OAAA;;AACA,WAAA,WAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,IAAA;AACA,KAxBA;AAyBA,IAAA,OAzBA,qBAyBA;AACA,WAAA,OAAA,GAAA,KAAA,QAAA,CACA,UAAA,CAAA,KAAA,OAAA,CADA,EAEA,UAAA,CAAA,KAAA,QAAA,CAFA,CAAA;AAIA,WAAA,KAAA,GAAA,KAAA,OAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KAhCA;AAiCA,IAAA,OAjCA,qBAiCA;AACA,WAAA,OAAA,GAAA,EAAA,EACA,KAAA,KAAA,GAAA,EADA,EAEA,KAAA,OAAA,GAAA,EAFA;AAGA,KArCA;AAsCA,IAAA,QAtCA,oBAsCA,IAtCA,EAsCA;AACA,UAAA,KAAA,eAAA,EAAA;AACA,aAAA,OAAA,GAAA,EAAA;AACA,aAAA,eAAA,GAAA,KAAA;AACA;;AACA,WAAA,OAAA,aAAA,KAAA,OAAA,SAAA,IAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,IAAA;AACA,KA7CA;AA8CA,IAAA,KA9CA,mBA8CA;AACA,UAAA,KAAA,OAAA,CAAA,OAAA,CAAA,GAAA,MAAA,CAAA,CAAA,EAAA;AACA,aAAA,QAAA,CAAA,GAAA;AACA;AACA;AAlDA;AAjBA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <section>\n      <div class=\"container\">\n        <div id=\"data-wrapper\">\n          <div>\n            <p class=\"answer\" v-html=\"this.total !== '' ? 'Ans = ' + this.total : ''\">Ans = {{ this.total }}</p>\n            <input v-model=\"displayData\" ref=\"email\" type=\"text\" />\n          </div>\n        </div>\n\n        <div id=\"buttons-wrapper\">\n          <div>\n            <div class=\"dark-grey two-btn\">\n              <button class=\"dark-grey\">Rad</button>\n              <button class=\"dark-grey disable\">Deg</button>\n            </div>\n            <button class=\"dark-grey\">x!</button>\n            <button class=\"dark-grey\">(</button>\n            <button class=\"dark-grey\">)</button>\n            <button class=\"dark-grey\">%</button>\n            <button class=\"dark-grey\" @click=\"clearFn()\">CE</button>\n          </div>\n\n          <div>\n            <button class=\"dark-grey\">Inv</button>\n            <button class=\"dark-grey\">sin</button>\n            <button class=\"dark-grey\">In</button>\n            <button class=\"light-grey num\" @click=\"appendFn('7')\">7</button>\n            <button class=\"light-grey num\" @click=\"appendFn('8')\">8</button>\n            <button class=\"light-grey num\" @click=\"appendFn('9')\">9</button>\n            <button class=\"dark-grey operator\" @click=\"divideFn('÷')\">÷</button>\n          </div>\n\n          <div>\n            <button class=\"dark-grey\">π</button>\n            <button class=\"dark-grey\">cos</button>\n            <button class=\"dark-grey\">log</button>\n            <button class=\"light-grey num\" @click=\"appendFn('4')\">4</button>\n            <button class=\"light-grey num\" @click=\"appendFn('5')\">5</button>\n            <button class=\"light-grey num\" @click=\"appendFn('6')\">6</button>\n            <button class=\"dark-grey operator\" @click=\"mulFn('×')\">×</button>\n          </div>\n\n          <div>\n            <button class=\"dark-grey\">e</button>\n            <button class=\"dark-grey\">tan</button>\n            <button class=\"dark-grey\">√</button>\n            <button class=\"light-grey num\" @click=\"appendFn('1')\">1</button>\n            <button class=\"light-grey num\" @click=\"appendFn('2')\">2</button>\n            <button class=\"light-grey num\" @click=\"appendFn('3')\">3</button>\n            <button class=\"dark-grey operator\" @click=\"subFn('-')\">−</button>\n          </div>\n\n          <div>\n            <button class=\"dark-grey\">Ans</button>\n            <button class=\"dark-grey\">EXP</button>\n            <button class=\"dark-grey\">x <sup>y</sup></button>\n            <button class=\"light-grey num\"  @click=\"appendFn('0')\">0</button>\n            <button class=\"light-grey operator\"  @click=\"dotFn('.')\">.</button>\n            <button class=\"equals\" @click=\"equalFn()\">=</button>\n            <button class=\"dark-grey operator\" @click=\"addFn('+')\">+</button>\n          </div>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      previous: null,\n      current: '',\n      getData: [],\n      total: '',\n      operator: null,\n      operatorClicked: false\n    }\n  },\n  computed: {\n    displayData(){\n      return this.getData.length === 0 ? '0' : this.getData.join('')\n    }\n  },\n  methods: {\n    setPrevious(){\n      this.previous = this.current\n      this.operatorClicked = true\n    },\n    addFn(data) {\n      this.operator = (a,b) => a + b\n      this.setPrevious()\n      this.getData.push(data)\n    },\n    subFn(data) {\n      this.operator = (a,b) => b - a\n      this.setPrevious()\n      this.getData.push(data)\n    },\n    mulFn(data) {\n      this.operator = (a,b) => a * b\n      this.setPrevious()\n      this.getData.push(data)\n    },\n    divideFn(data) {\n      this.operator = (a,b) => a / b\n      this.setPrevious()\n      this.getData.push(data)\n    },\n    equalFn() {\n      this.current = this.operator(\n        parseFloat(this.current),\n        parseFloat(this.previous)\n      )\n      this.total = this.current\n      this.previous = null\n    },\n    clearFn() {\n      this.current = '',\n      this.total = '',\n      this.getData = []\n    },\n    appendFn(data){\n      if(this.operatorClicked){\n        this.current = ''\n        this.operatorClicked = false\n      }\n      this.current = `${this.current}${data}`\n      this.getData.push(data)\n    },\n    dotFn() {\n      if(this.current.indexOf('.') === -1){\n        this.appendFn('.')\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\nhtml * {\n  font-family: 'Arial', sans-serif;\n}\n.container {\n  max-width: 800px;\n  margin: auto;\n}\n\n#data-wrapper {\n  border: 1px solid #ebebeb;\n  border-radius: 8px;\n  height: 72px;\n  padding: 10px 14px 5px 10px;\n  box-sizing: border-box;\n  display: flex;\n  justify-content: flex-end;\n  text-align: right;\n\n  > div {\n    width: 100%;\n  }\n\n  .answer{\n    margin: 0;\n    color: #70757a;\n    font-size: 13px;\n    min-height: 25px;\n  }\n\n  input {\n    -webkit-appearance: none;\n    outline: none;\n    border: none;\n    width: 100%;\n    font-size: 30px;\n    padding: 0;\n    text-align: right;\n  }\n}\n\n#buttons-wrapper{\n  > div {\n    display: flex;\n    grid-gap: 8px;\n    margin-top: 8px;\n  }\n\n  .two-btn {\n    min-width: 221px;\n    width: 100%;\n    display: flex;\n    position: relative;\n\n    &:before{\n      content: \"\";\n      position: absolute;\n      top: 10px;\n      bottom: 10px;\n      left: 50%;\n      border-left: 1px solid #70757a;\n      transform: translateX(-50%);\n    }\n  }\n\n  .dark-grey {\n    background: #dadce0;\n    color: #202124;\n    border: 1px solid #dadce0;\n    border-radius: 4px;\n\n    &:active {\n      border-color: #70757a;\n    }\n  }\n\n  .light-grey {\n    background: #f1f3f4;\n    color: #202124;\n    border: 1px solid #f1f3f4;\n    border-radius: 4px;\n\n    &:active {\n      border-color: #dadce0;\n    }\n  }\n\n  .equals{\n    background: #4285f4;\n    color: #fff;\n    border: 1px solid #4285f4;\n    border-radius: 4px;\n\n    &:active {\n      border-color: #1558d6;\n    }\n  }\n\n  .disable {\n    color: #70757a;\n  }\n  .num {\n    font-size: 14px;\n    font-weight: 400;\n  }\n  .operator {\n    font-size: 18px;\n    font-weight: 400;\n  }\n  button {\n    min-width: 25px;\n    max-width: 110px;\n    width: 100%;\n    height: 36px;\n    cursor: pointer;\n\n    &:active {\n      box-shadow: 0 1px 6px rgba(32, 33, 36, .28);\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src"}]}